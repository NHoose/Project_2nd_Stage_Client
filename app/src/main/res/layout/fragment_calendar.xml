<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="de.team_eduart.project2ndstage.KalenderFragment"
    android:id="@+id/Hallo">

    <TextView
        android:layout_width="264dp"
        android:layout_height="50dp"
        android:id="@+id/calendarWeekFrag"
        android:layout_gravity="top|center"
        android:layout_marginTop="30dp"
        android:textColor="@color/black"
        android:textSize="25dp"
        android:gravity="center_vertical|center_horizontal" />

    <Button
        android:id="@+id/addEventFrag"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_gravity="top|center"
        android:background="@drawable/ic_add_black_24dp"
        android:layout_marginLeft="330dp"
        android:layout_marginTop="30dp"/>

    <Button
        android:id="@+id/nextWeekFrag"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_gravity="top|center"
        android:layout_marginLeft="155dp"
        android:background="@drawable/ic_keyboard_arrow_right_grey600_24dp"
        android:layout_marginTop="30dp"
        />

    <Button
        android:id="@+id/prevWeekFrag"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_gravity="top|center"
        android:background="@drawable/ic_keyboard_arrow_left_grey600_24dp"
        android:layout_marginTop="30dp"
        android:layout_marginRight="155dp"/>


    <Button
        android:id="@+id/saveEventFrag"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_gravity="top|center"
        android:background="@drawable/ic_check_black_24dp"
        android:layout_marginLeft="330dp"
        android:layout_marginTop="30dp"
        android:visibility="gone"/>

    <TextView
        android:id="@+id/fragShow"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:text="Kalender"
        android:textColor="@color/black"
        android:textSize="25dp"
        android:gravity="center_vertical"
        android:layout_gravity="top|center"
        android:layout_marginTop="30dp"
        android:layout_marginRight="330dp"/>

    <Button
        android:id="@+id/settingsCal"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_gravity="top|center"
        android:background="@drawable/ic_settings_black_24dp"
        android:layout_marginTop="30dp"
        android:layout_marginLeft="380dp"/>

    <ListView
        android:id="@+id/calendarMonday"
        android:layout_width="30dp"
        android:layout_height="107dp"
        android:layout_gravity="left|top"
        android:layout_marginLeft="85dp"
        android:layout_marginTop="40dp"/>

    <ListView
        android:id="@+id/calendarThuesday"
        android:layout_width="30dp"
        android:layout_height="107dp"
        android:layout_gravity="left|top"
        android:layout_marginLeft="85dp"
        android:layout_marginTop="147dp"/>

    <ListView
        android:id="@+id/calendarWensday"
        android:layout_width="30dp"
        android:layout_height="107dp"
        android:layout_gravity="left|top"
        android:layout_marginLeft="85dp"
        android:layout_marginTop="254dp"/>

    <ListView
        android:id="@+id/calendarThurseday"
        android:layout_width="30dp"
        android:layout_height="107dp"
        android:layout_gravity="top|right"
        android:layout_marginRight="190dp"
        android:layout_marginTop="40dp"/>

    <ListView
        android:id="@+id/calendarFriday"
        android:layout_width="30dp"
        android:layout_height="107dp"
        android:layout_gravity="top|right"
        android:layout_marginRight="190dp"
        android:layout_marginTop="147dp"/>

    <TextView
        android:id="@+id/textViewMo"
        android:layout_width="90dp"
        android:layout_height="170dp"
        android:text="Montag"
        android:textSize="17dp"
        android:textColor="@color/black"
        android:layout_gravity="top|center"
        android:layout_marginRight="360dp"
        android:layout_marginTop="130dp" />

    <TextView
        android:id="@+id/textViewThue"
        android:layout_width="90dp"
        android:layout_height="170dp"
        android:text="Dienstag"
        android:textSize="17dp"
        android:textColor="@color/black"
        android:layout_gravity="top|center"
        android:layout_marginRight="360dp"
        android:layout_marginTop="300dp" />

    <TextView
        android:id="@+id/textViewWe"
        android:layout_width="90dp"
        android:layout_height="170dp"
        android:text="Mittwoch"
        android:textSize="17dp"
        android:textColor="@color/black"
        android:layout_gravity="top|center"
        android:layout_marginRight="360dp"
        android:layout_marginTop="470dp" />

    <TextView
        android:id="@+id/textViewTh"
        android:layout_width="90dp"
        android:layout_height="170dp"
        android:text="Donnerstag"
        android:textSize="17dp"
        android:textColor="@color/black"
        android:layout_gravity="top|center"
        android:layout_marginLeft="65dp"
        android:layout_marginTop="130dp" />

    <TextView
        android:id="@+id/textViewFr"
        android:layout_width="90dp"
        android:layout_height="170dp"
        android:text="Freitag"
        android:textSize="17dp"
        android:textColor="@color/black"
        android:layout_gravity="top|center"
        android:layout_marginLeft="65dp"
        android:layout_marginTop="300dp" />

    <TextView
        android:id="@+id/textViewSa"
        android:layout_width="200dp"
        android:layout_height="20dp"
        android:text="Samstag"
        android:textSize="17dp"
        android:textColor="@color/black"
        android:layout_gravity="top|center"
        android:layout_marginLeft="120dp"
        android:layout_marginTop="470dp" />

    <TextView
        android:id="@+id/textViewSu"
        android:layout_width="200dp"
        android:layout_height="20dp"
        android:text="Sonntag"
        android:textSize="17dp"
        android:textColor="@color/black"
        android:layout_gravity="top|center"
        android:layout_marginLeft="330dp"
        android:layout_marginTop="470dp" />

    <ImageView
        android:id="@+id/linesMo"
        android:layout_width="230dp"
        android:layout_height="170dp"
        android:src="@drawable/lines_pure"
        android:layout_gravity="center|top"
        android:layout_marginRight="140dp"
        android:layout_marginTop="130dp"/>

    <ImageView
        android:id="@+id/linesThue"
        android:layout_width="230dp"
        android:layout_height="170dp"
        android:src="@drawable/lines_pure"
        android:layout_gravity="top|center"
        android:layout_marginRight="140dp"
        android:layout_marginTop="300dp"/>

    <ImageView
        android:id="@+id/linesWe"
        android:layout_width="230dp"
        android:layout_height="170dp"
        android:src="@drawable/lines_pure"
        android:layout_gravity="top|center"
        android:layout_marginRight="140dp"
        android:layout_marginTop="470dp"/>
    <ImageView
        android:id="@+id/linesTh"
        android:layout_width="230dp"
        android:layout_height="170dp"
        android:src="@drawable/lines_pure"
        android:layout_gravity="top|center"
        android:layout_marginLeft="300dp"
        android:layout_marginTop="130dp"/>
    <ImageView
        android:id="@+id/linesFr"
        android:layout_width="230dp"
        android:layout_height="170dp"
        android:src="@drawable/lines_pure"
        android:layout_gravity="top|center"
        android:layout_marginLeft="300dp"
        android:layout_marginTop="300dp"/>

    <ImageView
        android:id="@+id/linesSa"
        android:layout_width="200dp"
        android:layout_height="170dp"
        android:src="@drawable/lines_pure"
        android:layout_gravity="top|center"
        android:layout_marginLeft="120dp"
        android:layout_marginTop="470dp"/>

    <ImageView
        android:id="@+id/linesSu"
        android:layout_width="200dp"
        android:layout_height="170dp"
        android:src="@drawable/lines_pure"
        android:layout_gravity="top|center"
        android:layout_marginLeft="330dp"
        android:layout_marginTop="470dp"/>

    <TextView
        android:id="@+id/dateMoFrag"
        android:textSize="15dp"
        android:layout_width="90dp"
        android:layout_height="20dp"
        android:layout_gravity="top|center"
        android:layout_marginRight="360dp"
        android:layout_marginTop="160dp"/>

    <TextView
        android:id="@+id/dateThueFrag"
        android:textSize="15dp"
        android:layout_width="90dp"
        android:layout_height="20dp"
        android:layout_gravity="top|center"
        android:layout_marginRight="360dp"
        android:layout_marginTop="330dp"/>

    <TextView
        android:id="@+id/dateWeFrag"
        android:textSize="15dp"
        android:layout_width="90dp"
        android:layout_height="20dp"
        android:layout_gravity="top|center"
        android:layout_marginRight="360dp"
        android:layout_marginTop="500dp"/>

    <TextView
        android:id="@+id/dateThFrag"
        android:textSize="15dp"
        android:layout_width="90dp"
        android:layout_height="20dp"
        android:layout_gravity="top|center"
        android:layout_marginLeft="65dp"
        android:layout_marginTop="160dp"/>


    <TextView
        android:id="@+id/dateFrFrag"
        android:textSize="15dp"
        android:layout_width="90dp"
        android:layout_height="20dp"
        android:layout_gravity="top|center"
        android:layout_marginLeft="65dp"
        android:layout_marginTop="330dp"/>

</FrameLayout>
